<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.swoosh">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".controller.FinishActivity" android:screenOrientation="fullSensor" />
        <activity android:name=".controller.SkillActivity" android:screenOrientation="fullSensor" />
        <activity android:name=".controller.BasicActivity" android:screenOrientation="fullSensor" />
        <activity android:name=".controller.LeagueActivity" android:screenOrientation="fullSensor"/>
        <activity android:name=".controller.MainActivity" android:screenOrientation="fullSensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>